{
  "name": "cloud_function",
  "displayName": "Cloud Function",
  "rules": [
    {
      "sources": [
        {
          "sourceType": "logs",
          "source": "resourceType",
          "condition": "$eq('cloud_function')"
        }
      ],
      "attributes": [
        {
          "key": "faas.id",
          "pattern": "labels.execution_id"
        },
        {
          "key": "content",
          "pattern": "textPayload"
        },
        {
          "key": "gcp.instance.name",
          "pattern": "resource.labels.function_name"
        }
      ]
    }
  ]
}