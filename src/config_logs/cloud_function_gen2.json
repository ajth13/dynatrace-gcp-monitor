{
  "name": "cloud_function",
  "displayName": "Cloud Function",
  "rules": [
    {
      "sources": [
        {
          "sourceType": "logs",
          "source": "managedBy",
          "condition": "$contains('cloudfunctions')"
        }
      ],
      "attributes": [
        {
          "key": "faas.id",
          "pattern": "join('', ['//cloudfunctions.googleapis.com/projects/', resource.labels.project_id, '/locations/', resource.labels.location, '/functions/', resource.labels.configuration_name])"
        },
        {
          "key": "faas.name",
          "pattern": "resource.labels.configuration_name"
        },
        {
          "key": "gcp.instance.name",
          "pattern": "resource.labels.configuration_name"
        },
        {
          "key": "content",
          "pattern": "@"
        }
      ]
    }
  ]
}